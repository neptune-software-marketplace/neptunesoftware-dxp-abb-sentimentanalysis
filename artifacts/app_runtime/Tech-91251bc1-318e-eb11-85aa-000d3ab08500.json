					"request": [],
					"response": [],
					"attributes": [
						{
							"attribute": "ajaxSuccess",
							"grouping": "Events",
							"value": "Anonymous Function",
							"script": [
								"==OBJECT STRING==",
								"",
								"if (xhr.responseJSON[0] === \"Positive\") {",
								"",
								"      for (i = 0; i < currBt.oParent.mAggregations.items.length; i++) {",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"btnSpeech\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNegative\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNeutral\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oPositive\")) currBt.oParent.mAggregations.items[i].setVisible(true);",
								"",
								"      }",
								"}",
								"if (xhr.responseJSON[0] === \"Neutral\") {",
								"      for (i = 0; i < currBt.oParent.mAggregations.items.length; i++) {",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"btnSpeech\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNegative\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNeutral\")) currBt.oParent.mAggregations.items[i].setVisible(true);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oPositive\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"",
								"      }",
								"}",
								"if (xhr.responseJSON[0] === \"Negative\") {",
								"      for (i = 0; i < currBt.oParent.mAggregations.items.length; i++) {",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"btnSpeech\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNegative\")) currBt.oParent.mAggregations.items[i].setVisible(true);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oNeutral\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"            if (currBt.oParent.mAggregations.items[i].sId.includes(\"oPositive\")) currBt.oParent.mAggregations.items[i].setVisible(false);",
								"",
								"      }",
								"}",
								"",
								"voicetext = \"\";"
							],
							"translation": []
						},
						{
							"attribute": "ajaxError",
							"grouping": "Events",
							"value": "Anonymous Function",
							"script": [
								"==OBJECT STRING==",
								"// Use MessageToast",
								"sap.m.MessageToast.show(\"fail calling sentiment\");"
							],
							"translation": []
						}
					],
					"data": []
				}
			]
		},
